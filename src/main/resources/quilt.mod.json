{
  "schema_version": 1,
  "quilt_loader": {
    "group": "de.ambertation",
    "id": "wunderreich",
    "version": "${version}",
    "metadata": {
      "name": "Wunderreich",
      "description": "A Mod with Quality of live improvements and some new gameplay machanics.",
      "contributors": {
        "Quiqueck (Frank)": "Owner"
      },
      "contact": {
        "homepage": "https://wunderreich.ambertation.de",
        "issues": "https://github.com/quiqueck/wunderreich/issues",
        "sources": "https://github.com/quiqueck/wunderreich"
      },
      "icon": "assets/wunderreich/icon.png",
      "license": "MIT"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        {
          "adapter": "default",
          "value": "de.ambertation.wunderreich.Wunderreich"
        }
      ],
      "client_init": [
        {
          "adapter": "default",
          "value": "de.ambertation.wunderreich.client.WunderreichClient"
        }
      ],
      "rei_server": [
        {
          "adapter": "default",
          "value": "de.ambertation.wunderreich.rei.ServerPlugin"
        }
      ],
      "rei_client": [
        {
          "adapter": "default",
          "value": "de.ambertation.wunderreich.rei.ClientPlugin"
        }
      ],
      "modmenu": [
        {
          "adapter": "default",
          "value": "de.ambertation.wunderreich.gui.modmenu.EntryPoint"
        }
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=2.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  },
  "access_widener": "wunderreich.accesswidener",
  "mixin": [
    "wunderreich.mixins.client.json",
    "wunderreich.mixins.common.json"
  ]
}
